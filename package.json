{
  "name": "go-lazy-mock",
	"displayName": "go-lazy-mock",
	"description": "go mock generate",
	"version": "0.0.2",
	"publisher": "nawath",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:go-lazy-mock.mockGen"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "go-lazy-mock.mockGen",
				"title": "Generate Mock"
			}
		],
		"menus": {
            "explorer/context": [{
                "when": "resourceLangId == go",
                "command": "go-lazy-mock.mockGen",
                "group": "MyGroup"
          }]
        }
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nawajar/go-lazy-mock.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
